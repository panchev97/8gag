{% extends 'base.html.twig' %}

{% block body %}
    <div style="text-align: center;">
        <div class="well">
            <h1>Upload Your Funniest Pictures</h1>
            <hr>
            <div class="container">
                <div class="col-md-4 col-md-offset-4">
                    {{ form_start(picturesForm) }}
                        {{ form_widget(picturesForm) }}
                        <button class="btn btn-primary" type="submit">Upload Photo</button>
                    {{ form_end(picturesForm) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}